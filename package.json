{
  "name": "dishdash",
  "version": "1.7.8",
  "codepush_version": "1.7",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:stg:win": "adb reverse tcp:8081 tcp:8081 && SET ENVFILE=.env.staging && react-native run-android --variant=stagingDebug --appIdSuffix=stg",
    "android:stg": "ENVFILE=.env.staging && react-native run-android --variant=stagingDebug --appIdSuffix=stg",
    "android:prod": "ENVFILE=.env.prod && react-native run-android --variant=prodDebug",
    "ios": "react-native run-ios",
    "ios2": "react-native run-ios --simulator='iPhone 14 Pro Max'",
    "ios3": "react-native run-ios --simulator='iPhone 13 Pro Max'",
    "ios:staging": "ENVFILE=.env.staging && arch -x86_64 react-native run-ios --scheme DishDash-Staging  --configuration DebugStaging",
    "ios:prod": "ENVFILE=.env.prod && arch -x86_64 react-native run-ios --scheme DishDash  --configuration Debug",
    "ios-device": "react-native run-ios --device",
    "clean-ios": "cd ios && xcodebuild clean && cd ../",
    "start": "react-native start --reset-cache --port 8898",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "patch-package",
    "codepush": "sh ./codepush.sh",
    "m1-pod-install": "cd ./ios && arch -x86_64 pod install",
    "build-android": "cd android && ./gradlew assembleDebug",
    "release-android": "cp ./.env.prod ./.env && cd android && ./gradlew clean && ./gradlew assembleRelease"
  },
